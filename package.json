    "postinstall": "husky install",
    "debug": "ndb src/index.ts",
    "lint:ts": "eslint . --ext .ts",
    "todo": "leasot **/** .**/** -i node_modules -S -x",
    "test": "nodemon --exec node_modules/.bin/babel-node src/index.js"
